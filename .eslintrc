{
  "env": {
    "browser": true
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "rules": {
    "new-cap": ["error", { "capIsNew": false }],
    "import/prefer-default-export": "off",
    "no-console": "off",
    "semi": "error"
  }
}
